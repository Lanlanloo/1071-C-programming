#include<stdio.h>

int main(){
	int n1,n2;
	int ans1,ans2,ans3,ans4,ans5;
	printf("Enter two integers:\n");
	scanf("%d %d",&n1,&n2);
	ans1=n1+n2;
	ans2=n1-n2;
	ans3=n1*n2;
	ans4=n1/n2;
	ans5=n1%n2;
	printf("%d+%d=%d\n",n1,n2,ans1);
	printf("%d-%d=%d\n",n1,n2,ans2);
	printf("%d*%d=%d\n",n1,n2,ans3);
	printf("%d/%d=%d\n",n1,n2,ans4);
	printf("%d%%%d=%d\n",n1,n2,ans5);
	return 0;
}
